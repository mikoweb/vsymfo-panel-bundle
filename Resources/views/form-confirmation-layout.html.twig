    <fieldset class="btn-group" role="group">
        {{ form_widget(form.confirm, {'attr': {'class': 'btn btn-success'}}) }}
        {{ form_widget(form.cancel, {'attr': {'class': 'btn btn-default'}}) }}
    </fieldset>
{% endblock %}
